
(deftype SomeVal (any-of Integer
                         String))

(defn f [x y]
  ! x SomeVal

  (or (and (do
             (print-err 'y (inc y))
             (instance? Integer x))
           (maybe (inc x)))
      (and (do
             (print-err 'y (+ 4 y))
             (instance? String x))
           (maybe (subs x 1)))))

(main [_]
  (print-err "1)" (f 4 9))
  (print-err "2)" (f "212" 3))
  (print-err "3)" (f "212" "34")))
