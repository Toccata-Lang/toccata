
(deftype OneType [x]
  Stringable
  (string-list [_] (list "<OneType " (str x) ">")))

(deftype AnotherType [x z]
  Stringable
  (string-list [_] (list "<AnotherType " (str z) ">")))

(deftype BothTypes (any-of OneType
                           AnotherType))

(defn only-one [x]
  ! x OneType

  x)

(defn bad [x]
  ! x BothTypes

  (only-one x))

(main [_]
  (bad (AnotherType 'x 'z))
  (print-err 'FAIL))
