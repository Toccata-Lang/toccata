
(def Bands (all-of (min-value 4)
                   Integer
                   (max-value 8)))

;; TODO: error message should tell where 'Bands' is defined at
;; And should indicate what part of the constraint is violated

(defn pr-bands [n]
  (assert (instance? Bands n))

  (print-err 'bands n))

(main [_]
  (pr-bands 4)
  (pr-bands 10)

  (print-err 'FAIL))
