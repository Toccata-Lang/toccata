
(defn f [x]
  (map (maybe 3)
       (fn [_]
         (inc x))))

(defn g [x]
  (for [y (f x)]
    (do
      ;; TODO: this assertion should somehow limit the ParamConstraint of 'y' to an Integer
      (assert (instance? Integer y))
      y)))

(defn h [x]
  (for [_ (maybe 3)]
    (symbol x)))

(defn i [x]
  (for [y (h x)]
    i))

(defn j [x]
  (for [y (g x)
        _ (i y)]
    x))

(main [_]
  (print-err (j 8)))
