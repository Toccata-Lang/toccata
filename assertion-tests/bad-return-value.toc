(defprotocol C
  (eic [_]
    (assert-result r (instance? Vector r))
    []))

(deftype IC [items]
  Stringable
  (string-list [_]
    (list "<IC " (str items) ">")))

(deftype MC [cs]
  C
  (eic [mc]
    (-> (.cs mc)
        (some (partial instance? IC))
        (map .items))))

