(defn fresh [x]
  (reify
    Stringable
    (string-list [_]
      (list (str x)))

    Container
    (map [this f]
      this)

    (map [this f embed]
      (embed this))

    Eq
    (=* [x y]
      (identical x y))

    ast/AST
    (ast/vars [_] {})

    Evaluation
    (redex [this defs hist]
      this)

    DotOut
    (grph-up [_]
      (let [node-id (gensym (str x))]
        (println node-id (str "[label=\"" x "\"]"))
        (se/state-error (str node-id ":n"))))))

