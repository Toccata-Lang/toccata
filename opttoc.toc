  ;;                                          (hylo-env pre-eval eval env arg)))]
  ;;     (cond (or (= f grph)
  ;;               (every new-args (partial instance? CoreType)))
  ;;           (apply f new-args)

  ;;           (ast/call-ast f new-args))))

