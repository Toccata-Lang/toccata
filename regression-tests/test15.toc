

(defn integer-str [n]
  (inline String "
   return(integer_str(n_0));
"))

(main [_]
  (let [xx 10
        xs (list (integer-str xx))
        z "second msg\n"
        f1 (fn f1 [y]
             (pr* y)
             (pr* z)
             (let [[x] xs]
               (pr* x)
               (pr* " ")
               (pr* x)
               (pr* "\n")
               (pr* z)))
        [x] xs]
    (pr* "x: ")
    (pr* x)
    (pr* "\n")
    (pr* "z: ")
    (pr* z)
    (f1 "Howdy\n")))


