
(def vect1 (vector 1 2 3))
(def list1 (list 4 5 6))

(defn integer-str [n]
  (inline String "
   return(integer_str(n_0));
"))

(defn f1 [[x y z]]
  (pr* (integer-str x))
  (pr* " ")
  (pr* (integer-str y))
  (pr* " ")
  (pr* (integer-str z))
  (pr* "\n"))

(main [_]
  (let [[x y] vect1]
    (pr* (integer-str x))
    (pr* " ")
    (pr* (integer-str y))
    (pr* " "))
  (let [[a b c] list1]
    (pr* (integer-str a))
    (pr* " ")
    (pr* (integer-str b))
    (pr* " ")
    (pr* (integer-str c))
    (pr* "\n"))
  (pr* "Now for the local call sites\n")
  (let [[x y z] (vector 1 2 3)]
    (pr* (integer-str x))
    (pr* " ")
    (pr* (integer-str y))
    (pr* " ")
    (pr* (integer-str z))
    (pr* " "))
  (let [[a b c] (list 4 5 6)]
    (pr* (integer-str a))
    (pr* " ")
    (pr* (integer-str b))
    (pr* " ")
    (pr* (integer-str c))
    (pr* "\n"))
  (pr* "Function destructuring\n")
  (f1 vect1)
  (f1 list1)
  (pr* "Vectors and Lists! Oh my!\n"))
