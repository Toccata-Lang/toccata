(defn get-in* [m path]
  (assert (instance? List path))
  (or (and (empty? path)
           (maybe m))
      (for [k (first path)
            sub-m (get m k)
            v (get-in* sub-m (rest path))]
        v)))

(defn get-in [m path]
  (and (< 0 (count path))
       (get-in* m (seq path))))

(defn update-in [m path f]
  (or (and (empty? path)
           (maybe m))
      (and (= 1 (count path))
           (for [k (first path)
                 v (get m k)]
             (assoc m k (f v))))
      (for [k (first path)
            sub-m (get m k)
            new-m (update-in sub-m (rest path) f)]
        (assoc m k new-m))))

