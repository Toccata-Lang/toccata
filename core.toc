(defn assoc-in* [m path v]
  (assert (instance? List path))
  (or (and (empty? path)
           (maybe m))
      (for [_ (= 1 (count path))
            k (first path)]
        (assoc m k v))
      (for [k (first path)
            sub-m (or (get m k)
                      (maybe {}))
            new-m (assoc-in* sub-m (rest path) v)]
        (assoc m k new-m))))

